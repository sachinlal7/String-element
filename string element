#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    char arr[1000];
    int i,s=0;
    scanf("%[^\n]s",arr);
    for(i=0;arr[i]!='\0';i++)
    {
        if(arr[i]=='r'&& arr[i+1]=='a')
            s=1;
           
    }
    if(s==1)
        printf("Yes");
    else
        printf("No");
    return 0;
}
